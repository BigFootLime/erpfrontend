    "@types/recharts": "^1.8.29",
    "axios": "^1.7.4",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "concurrently": "^8.0.1",
    "electron-updater": "^6.1.7",
    "express": "^4.19.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.14",
    "pg": "^8.12.0",
    "sequelize": "^6.37.3",
    "framer-motion": "^11.3.20",
    "input-otp": "^1.2.4",
    "lucide-react": "^0.400.0",
    "react-hook-form": "^7.52.1",
    "react-router-dom": "^6.24.1",
    "react-transition-group": "^4.4.5",
